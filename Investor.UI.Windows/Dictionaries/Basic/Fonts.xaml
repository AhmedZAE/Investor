<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Investor.UI.Windows"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <!--#region Font Families -->
    
    <FontFamily x:Key="FontAwesome.Regular">pack://appliction;,,,/Fonts/#Font Awesome 5 Free Regular</FontFamily>
    <FontFamily x:Key="FontAwesome.Solid">pack://appliction;,,,/Fonts/#Font Awesome 5 Free Solid</FontFamily>
    <FontFamily x:Key="FontAwesome.Brand">pack://appliction;,,,/Fonts/#Font Awesome 5 Brands Regular</FontFamily>

    <!--#endregion-->


    <!--#region Font Sizes -->
    
    <system:Double x:Key="FontSizeSmall">14</system:Double>
    <system:Double x:Key="FontSizeAverage">18</system:Double>
    <system:Double x:Key="FontSizeRegular">20</system:Double>
    <system:Double x:Key="FontSizeLarge">30</system:Double>
    
    <!--#endregion-->


    <!--#region Font Icons -->

    <system:String x:Key="FontAwesome.CloseIcon">&#xf00d;</system:String>
    <system:String x:Key="FontAwesome.MaximizeIcon">&#xf2d0;</system:String>
    <system:String x:Key="FontAwesome.UnMaximizeIcon">&#xf2d2;</system:String>
    <system:String x:Key="FontAwesome.MinimizeIcon">&#xf2d1;</system:String>
    
    <!--#endregion-->

</ResourceDictionary>