<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Investor.UI.Windows">


    <!--#region Main Themes -->
    
    <Color x:Key="LightMode.MainTheme">#000000</Color>
    <SolidColorBrush x:Key="LightMode.MainTheme.Brush" Color="{StaticResource LightMode.MainTheme}" />

    <Color x:Key="DarkMode.MainTheme">#ffffff</Color>
    <SolidColorBrush x:Key="DarkMode.MainTheme.Brush" Color="{StaticResource DarkMode.MainTheme}" />
    
    <!--#endregion-->

    
    
</ResourceDictionary>